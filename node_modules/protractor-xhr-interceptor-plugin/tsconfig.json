{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "allowJs": true,
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "types": [
      "protractor",
      "jasmine"
    ],
    "skipLibCheck": true,
    "noImplicitAny": false,
    "esModuleInterop": true
  },
  "include": [
    "src",
    "index.ts"
  ],
  "exclude": [
    "node_modules",
    "./node_modules",
    "./node_modules/*",
    "./spec/*"
  ]
}